    fun print(ref A : int[3][3]; N : int) : nothing
        var i,j : int;
    {
        i <- 0;
        while (i < N-1) do {
            j <- 0;
            while (j < N-1) do {
                writeInteger(A[i][j]);
                writeString(" ");
                j <- j+1;
            }
            writeInteger(A[i][N-1]);
            writeString("\n");
            i <- i+1;
        }
        i <- N-1;
        j <- 0;
        while (j < N-1) do {
            writeInteger(A[i][j]);
            writeString(" ");
            j <- j+1;
        }
        writeInteger(A[i][N-1]);
        writeString("\n");
    }
    fun matmult(ref A,B,C : int[3][3]; N : int) : nothing
        var i,j,k,sum : int;
    {
        i <- 0;
        while (i < N) do {
            j <- 0;
            while (j < N) do {
                k   <- 0;
                sum <- 0;
                while (k < N) do {
                    sum <- sum + A[i][k]*B[k][j];
                    k <- k+1;
                }
                C[i][j] <- sum;
                j <- j+1;
            }
            i <- i+1;
        }
    }
