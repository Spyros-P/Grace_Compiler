source_filename = "grace program"

@a = private global i32 0

define i32 @main() {
entry:
  %lval_tmp = load i32, i32* @a, align 4
  %0 = call i32 @"foo_$1"(i32 %lval_tmp)
  %lval_tmp1 = load i32, i32* @a, align 4
  %1 = call i32 @"bar_$3"(i32 %lval_tmp1)
  %addtmp = add i32 %0, %1
  store i32 %addtmp, i32* @a, align 4
  ret i32 0
}

define i32 @"foo_$1"(i32 %0) {
entry:
  %a = alloca i32, align 4
  store i32 %0, i32* %a, align 4
  %lval_tmp = load i32, i32* %a, align 4
  %1 = call i32 @"bar_$2"(i32 %lval_tmp)
  ret i32 %1

after_ret:                                        ; No predecessors!
  ret i32 0
}

define i32 @"bar_$2"(i32 %0) {
entry:
  %b = alloca i32, align 4
  store i32 %0, i32* %b, align 4
  ret i32 5

after_ret:                                        ; No predecessors!
  ret i32 0
}

define i32 @"bar_$3"(i32 %0) {
entry:
  %a = alloca i32, align 4
  store i32 %0, i32* %a, align 4
  ret i32 6

after_ret:                                        ; No predecessors!
  ret i32 0
}
