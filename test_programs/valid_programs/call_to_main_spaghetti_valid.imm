; ModuleID = 'grace program'
source_filename = "grace program"

@"Hello bruh!\0A" = private constant [13 x i8] c"Hello bruh!\0A\00", align 1
@"bruh\0A" = private constant [6 x i8] c"bruh\0A\00", align 1
@test = private global i32 0
@"Hello world!\0A" = private constant [14 x i8] c"Hello world!\0A\00", align 1

define i32 @main() {
entry:
  call void @writeString(i8* getelementptr inbounds ([14 x i8], [14 x i8]* @"Hello world!\0A", i32 0, i32 0))
  call void @"bruh_$1"()
  ret i32 0
}

declare void @writeString(i8*)

define void @"bruh_$1"() {
entry:
  call void @writeString(i8* getelementptr inbounds ([6 x i8], [6 x i8]* @"bruh\0A", i32 0, i32 0))
  br label %cond

cond:                                             ; preds = %entry
  br i1 true, label %then, label %else

then:                                             ; preds = %cond
  call void @"hello_$2"()
  br label %after

else:                                             ; preds = %cond
  ret void

after:                                            ; preds = %after_ret, %then
  ret void

after_ret:                                        ; No predecessors!
  br label %after
}

define void @"hello_$2"() {
entry:
  call void @writeString(i8* getelementptr inbounds ([13 x i8], [13 x i8]* @"Hello bruh!\0A", i32 0, i32 0))
  call void @"hello_$2"()
  ret void
}
